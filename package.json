{
  "name": "my-good-reads",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.3.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "indent": [
        "warn",
        2
      ],
      "react/jsx-indent": [
        "warn",
        2
      ],
      "react/jsx-indent-props": [
        "warn",
        2
      ],
      "semi": "warn",
      "eol-last": "warn",
      "quotes": [
        "warn",
        "single"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/sinon": "^9.0.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "react-scripts": "3.4.1",
    "sinon": "^9.0.3",
    "typescript": "~3.7.2",
    "node-sass": "^4.14.1"
  }
}
